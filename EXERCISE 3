package selfProgramming;

public class Exercise3 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int k=20;
		int j=20;
		int l;
		
		while (k>10) {
			System.out.println("k = " + k);
			k--;
		}
		
		do {
			System.out.println("j = " + j);
			j--;
		}
		while (j>10);
		
		for (l=20; l>10; l--) {
			System.out.println("l = " + l);
		}

	}

}
